<body>
    <h1>Array Methods</h1>
    <p id="array-methods"></p>
    <script>
        let numbers = [1, 2, 3, 4, 5];
        numbers.push(6);
        let squaredNumbers = numbers.map(num => num * num);
        document.getElementById('array-methods').textContent = `Array after push: [${numbers}], Squared Numbers: [${squaredNumbers}]`;
    </script>
</body>
</html>
